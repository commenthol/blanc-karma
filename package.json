{
  "name": "blanc-karma",
  "version": "0.1.0",
  "description": "boilerplate using webpack and karma",
  "keywords": [
    "module",
    "boilerplate"
  ],
  "homepage": "https://github.com/commenthol/blanc-karma",
  "bugs": {
    "url": "https://github.com/commenthol/blanc-karma/issues"
  },
  "license": "ISC",
  "author": "commenthol <commenthol@gmail.com>",
  "main": "src/index.js",
  "directories": {
    "doc": "doc",
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/blanc-karma.git"
  },
  "scripts": {
    "build": "webpack",
    "lint": "eslint --ignore-path .gitignore '**/*.js'",
    "test": "karma start",
    "tdd": "karma start --auto-watch --no-single-run",
    "readme": "markedpp --githubid -i README.md -o README.md"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.0.5",
    "webpack": "^4.5.0",
    "webpack-merge": "^4.1.2"
  },
  "__phantom": {
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.4"
  },
  "private": true,
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "no-console": 2
    }
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": []
  }
}
